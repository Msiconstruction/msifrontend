{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "msiconstruction.co.uk"
        }
      ],
      "destination": "https://www.msiconstruction.co.uk/:path*",
      "permanent": true
    }
  ]
}
